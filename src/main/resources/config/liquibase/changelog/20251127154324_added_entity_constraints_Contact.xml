<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd
                        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">
    <!--
        Added the constraints for entity Contact.
    -->
    <changeSet id="20251127154324-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="tenant_id"
                                 baseTableName="contact"
                                 constraintName="fk_contact__tenant_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tenant"
                                 />

        <addForeignKeyConstraint baseColumnNames="customer_id"
                                 baseTableName="contact"
                                 constraintName="fk_contact__customer_id"
                                 referencedColumnNames="id"
                                 referencedTableName="customer"
                                 />

        <addForeignKeyConstraint baseColumnNames="address_id"
                                 baseTableName="contact"
                                 constraintName="fk_contact__address_id"
                                 referencedColumnNames="id"
                                 referencedTableName="address"
                                 />

        <addForeignKeyConstraint baseColumnNames="owner_user_id"
                                 baseTableName="contact"
                                 constraintName="fk_contact__owner_user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="mt_user"
                                 />
    </changeSet>
</databaseChangeLog>
