<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd
                        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">
    <!--
        Added the constraints for entity Deal.
    -->
    <changeSet id="20251127154330-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="tenant_id"
                                 baseTableName="deal"
                                 constraintName="fk_deal__tenant_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tenant"
                                 />

        <addForeignKeyConstraint baseColumnNames="customer_id"
                                 baseTableName="deal"
                                 constraintName="fk_deal__customer_id"
                                 referencedColumnNames="id"
                                 referencedTableName="customer"
                                 />

        <addForeignKeyConstraint baseColumnNames="contact_id"
                                 baseTableName="deal"
                                 constraintName="fk_deal__contact_id"
                                 referencedColumnNames="id"
                                 referencedTableName="contact"
                                 />

        <addForeignKeyConstraint baseColumnNames="lead_id"
                                 baseTableName="deal"
                                 constraintName="fk_deal__lead_id"
                                 referencedColumnNames="id"
                                 referencedTableName="lead"
                                 />
    </changeSet>
</databaseChangeLog>
