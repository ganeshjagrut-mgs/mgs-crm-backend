<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd
                        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">
    <!--
        Added the constraints for entity Customer.
    -->
    <changeSet id="20251127154323-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="tenant_id"
                                 baseTableName="customer"
                                 constraintName="fk_customer__tenant_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tenant"
                                 />

        <addForeignKeyConstraint baseColumnNames="department_id"
                                 baseTableName="customer"
                                 constraintName="fk_customer__department_id"
                                 referencedColumnNames="id"
                                 referencedTableName="department"
                                 />

        <addForeignKeyConstraint baseColumnNames="billing_address_id"
                                 baseTableName="customer"
                                 constraintName="fk_customer__billing_address_id"
                                 referencedColumnNames="id"
                                 referencedTableName="address"
                                 />

        <addForeignKeyConstraint baseColumnNames="shipping_address_id"
                                 baseTableName="customer"
                                 constraintName="fk_customer__shipping_address_id"
                                 referencedColumnNames="id"
                                 referencedTableName="address"
                                 />

        <addForeignKeyConstraint baseColumnNames="primary_contact_id"
                                 baseTableName="customer"
                                 constraintName="fk_customer__primary_contact_id"
                                 referencedColumnNames="id"
                                 referencedTableName="contact"
                                 />
    </changeSet>
</databaseChangeLog>
