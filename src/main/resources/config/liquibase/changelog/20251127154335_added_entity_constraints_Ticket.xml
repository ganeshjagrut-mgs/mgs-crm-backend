<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd
                        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">
    <!--
        Added the constraints for entity Ticket.
    -->
    <changeSet id="20251127154335-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="tenant_id"
                                 baseTableName="ticket"
                                 constraintName="fk_ticket__tenant_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tenant"
                                 />

        <addForeignKeyConstraint baseColumnNames="customer_id"
                                 baseTableName="ticket"
                                 constraintName="fk_ticket__customer_id"
                                 referencedColumnNames="id"
                                 referencedTableName="customer"
                                 />

        <addForeignKeyConstraint baseColumnNames="contact_id"
                                 baseTableName="ticket"
                                 constraintName="fk_ticket__contact_id"
                                 referencedColumnNames="id"
                                 referencedTableName="contact"
                                 />

        <addForeignKeyConstraint baseColumnNames="assigned_to_id"
                                 baseTableName="ticket"
                                 constraintName="fk_ticket__assigned_to_id"
                                 referencedColumnNames="id"
                                 referencedTableName="mt_user"
                                 />
    </changeSet>
</databaseChangeLog>
