<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd
                        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">
    <!--
        Added the constraints for entity QuotationItem.
    -->
    <changeSet id="20251127154332-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="tenant_id"
                                 baseTableName="quotation_item"
                                 constraintName="fk_quotation_item__tenant_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tenant"
                                 />

        <addForeignKeyConstraint baseColumnNames="quotation_id"
                                 baseTableName="quotation_item"
                                 constraintName="fk_quotation_item__quotation_id"
                                 referencedColumnNames="id"
                                 referencedTableName="quotation"
                                 />

        <addForeignKeyConstraint baseColumnNames="product_id"
                                 baseTableName="quotation_item"
                                 constraintName="fk_quotation_item__product_id"
                                 referencedColumnNames="id"
                                 referencedTableName="product"
                                 />
    </changeSet>
</databaseChangeLog>
