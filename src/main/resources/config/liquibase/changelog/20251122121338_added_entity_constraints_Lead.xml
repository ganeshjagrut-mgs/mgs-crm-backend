                                 baseTableName="lead"
                                 constraintName="fk_lead__lead_source_id"
                                 referencedColumnNames="id"
                                 referencedTableName="master_static_type"
                                 />

        <addForeignKeyConstraint baseColumnNames="industry_type_id"
                                 baseTableName="lead"
                                 constraintName="fk_lead__industry_type_id"
                                 referencedColumnNames="id"
                                 referencedTableName="master_static_type"
                                 />

        <addForeignKeyConstraint baseColumnNames="lead_status_id"
                                 baseTableName="lead"
                                 constraintName="fk_lead__lead_status_id"
                                 referencedColumnNames="id"
                                 referencedTableName="master_static_type"
                                 />
    </changeSet>
</databaseChangeLog>
